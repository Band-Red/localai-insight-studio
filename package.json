{
  "name": "local-ai-studio",
  "version": "6.0.0",
  "description": "المحرك السيادي المستقل لمعالجة البيانات والذكاء الاصطناعي المحلي",
  "main": "dist/main.js",
  "scripts": {
    "start": "electron .",
    "dev": "concurrently \"npm run build:watch\" \"electron .\"",
    "build": "webpack --config webpack.config.js",
    "build:watch": "webpack --watch --config webpack.config.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dist": "npm run build && electron-builder",
    "pack": "npm run build && electron-builder --dir"
  },
  "build": {
    "appId": "com.localai.studio",
    "productName": "LocalAI Insight Studio",
    "directories": {
      "output": "release"
    },
    "win": {
      "target": [
        "nsis",
        "zip"
      ]
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    },
    "files": [
      "dist/**/*",
      "package.json",
      "index.html",
      "src/renderer/index.css"
    ],
    "extraResources": [
      {
        "from": "bin",
        "to": "bin",
        "filter": [
          "**/*"
        ]
      }
    ]
  },
  "keywords": [
    "electron",
    "react",
    "local-ai",
    "obsidian",
    "sovereign-engine"
  ],
  "author": "LocalAI Studio Team",
  "license": "MIT",
  "dependencies": {
    "@types/node-os-utils": "^1.3.4",
    "chokidar": "^3.5.3",
    "dotenv": "^17.3.1",
    "lucide-react": "^0.284.0",
    "pdf-parse": "^1.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "recharts": "^3.7.0"
  },
  "devDependencies": {
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "concurrently": "^8.2.1",
    "css-loader": "^6.8.1",
    "electron": "^26.2.1",
    "electron-builder": "^26.8.1",
    "html-webpack-plugin": "^5.6.6",
    "style-loader": "^3.3.3",
    "ts-loader": "^9.4.4",
    "typescript": "^5.2.2",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-node-externals": "^3.0.0"
  }
}